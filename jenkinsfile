
pipeline {
    agent any
tools {
maven "Maven"
}
    stages {
        stage('gitcheckout') {
            steps {
                 
                    git 'https://github.com/ALISAYYAD09/maven-web-application.git'

      }
        }
      stage('junit test') {
            steps {
                script{
                      sh 'mvn test'
                   }
            }
        }
       stage('integration test') {
            steps {
                script{
                      sh 'mvn verify -DskipUnitTests'
                   }
            }
        }
        stage('build') {
            steps {
                script{
                      sh 'mvn clean package'
                   }
            }
        }

    }
}

